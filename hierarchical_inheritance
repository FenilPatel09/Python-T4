class Father:
    
    def __init__(self):
        print("Father constructor")
        
    def showF(self):
        print("Father method")
        
class Son(Father):
    
    def showS(self):
        print("Son method ")

class Dauter(Father):
    
    def showD(self):
        print("Dauter method")

g=Dauter()
g.showD()
g.showF()
